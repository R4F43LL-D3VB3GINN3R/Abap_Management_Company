MODULE MENU_INIT200 INPUT.

  CASE OKCODE200.
    WHEN 'FCT_INSERTMENU200'.
      CALL SCREEN '300'.
    WHEN 'FCT_EXIT200'.
      LEAVE PROGRAM.
  ENDCASE.

ENDMODULE.
