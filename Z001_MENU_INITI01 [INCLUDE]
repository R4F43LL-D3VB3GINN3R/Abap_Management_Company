MODULE MENU_INIT INPUT.

CASE OKCODE200.
  WHEN 'FCT_INSERTMENU'.
    CALL SCREEN '300'.
  WHEN 'FCT_EXIT200'.
    LEAVE PROGRAM.
ENDCASE.

ENDMODULE.
